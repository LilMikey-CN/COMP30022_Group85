{
    "name": "do-spaces-crud-api",
    "version": "1.0.0",
    "description": "Simple CRUD API for Digital Ocean Spaces",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:unit": "jest unit.test.js --testTimeout=10000",
