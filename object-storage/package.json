        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "multer-s3": "^3.0.1",
        "node-fetch": "^2.7.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.13",
        "@types/supertest": "^6.0.2",
        "jest": "^30.0.0",
        "nodemon": "^3.1.10",
        "supertest": "^7.0.0"
    }
}
